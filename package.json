{
  "name": "vee-validate",
  "version": "2.0.0-rc.17",
  "description": "Simple Vue.js input validation plugin",
  "module": "dist/vee-validate.esm.js",
  "unpkg": "dist/vee-validate.js",
  "main": "dist/vee-validate.js",
  "typings": "types/index.d.ts",
  "homepage": "http://vee-validate.logaretm.com",
  "repository": "https://github.com/baianat/vee-validate",
  "scripts": {
    "test": "npm run lint && jest",
    "lint": "eslint ./src",
    "build:locale": "node build/locale.js",
    "build:bundle": "node build/main.js",
    "build": "node build/main.js && node build/locale.js",
    "cover": "jest --coverage"
  },
  "keywords": [
    "VueJS",
    "Vue",
    "validation",
    "validator",
    "inputs",
    "form"
  ],
  "jest": {
    "testMatch": [
      "**/tests/**/*.js"
    ],
    "testPathIgnorePatterns": [
      "/helpers/"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/minimal.js",
      "!src/messages.js",
      "!src/plugins/date/messages.js"
    ]
  },
  "author": "Abdelrahman Awad <logaretm1@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "chalk": "^2.1.0",
    "cross-env": "^5.0.5",
    "date-fns": "2.0.0-alpha.1",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^20.0.4",
    "regenerator-runtime": "^0.11.0",
    "rollup": "^0.49.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "uglify-js": "^3.0.28",
    "validator": "^8.1.0",
    "vue": "^2.4.2"
  },
  "peerDependencies": {
    "vue": ">= 2.2.0"
  },
  "dependencies": {}
}
